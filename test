    public static boolean checkResultXmlFile(String resourceFolderPath, int favoriteId, String resultXmlFileName) {
        boolean result = false;
        File resultFile = new File(resourceFolderPath + File.separator + WatchFaceSharedConstant.ID + favoriteId + File.separator + resultXmlFileName);
        if(!resultFile.exists()) {
            WFLog.d(TAG, "checkResultXmlFile resultFile existed : " + resultFile.getAbsolutePath());
            File resultDefaultFile = new File(resourceFolderPath + File.separator + resultXmlFileName);
            if(resultDefaultFile.exists()) {
                WFLog.d(TAG, "checkResultXmlFile resultDefaultFile : " + resultDefaultFile.getAbsolutePath());
                result = SharedFileUtils.copyFile(resultDefaultFile, resultFile);
            } else
                WFLog.d(TAG, "checkResultXmlFile resultDefaultFile not existed");
        } else
            result = true;
        WFLog.d(TAG, "checkResultXmlFile result : " + result);
        return result;
    }
